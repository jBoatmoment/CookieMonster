{
    "manifest_version": 3,
    "name": "Cookie Browser Manager",
    "version": "1.0",
    "description": "A simple UI for toggling cookie management.",
    "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
      }
    },
    "background": {
      "service_worker": "popup/popup_background.js"
    },
    "permissions": [
      "storage",
      "webRequest",
      "webRequestBlocking",
      "cookies",
      "notifications",
      "contextMenus",
      "<all_urls>"
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }
  